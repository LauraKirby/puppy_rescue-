

<%= form_for @puppy do |f|%>
  <div>
    <%= f.label :name%>
    <%= f.text_field :name, autofocus: true%>
  </div>
  <div>
    <%= f.label :image%>
    <%= f.text_field :image%>
  </div>
  <div>
    <%= f.label :breed%>
    <%= f.text_field :breed%>
  </div>
  <div>
    <%= f.label :bio%>
    <%= f.text_field :bio%>
  </div>
  <div>
    <%= f.label :age%>
    <%= f.text_field :age%>
  </div>
  <div>
    <%= f.submit%>
  </div>
<% end %>